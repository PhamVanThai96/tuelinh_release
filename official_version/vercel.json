{
    "version": 2,
    "builds": [
        {
            "src": "tuelinh_real_estate/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles",
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "tuelinh_real_estate/wsgi.py"
        }
    ]
}